server_host = ldaps://{{ ldapHost }}:636
server_port = 636
search_base = cn=groups,cn=accounts,{{baseDN}}
query_filter = (cn=%u)
result_attribute = mail
special_result_attribute = member
bind = yes
bind_dn = uid={{ bindUser }},cn=users,cn=accounts,{{baseDN}}
bind_pw = {{ bindPw }}
version = 3
tls_require_cert = yes
tls_ca_cert_file = /etc/ipa/ca.crt
